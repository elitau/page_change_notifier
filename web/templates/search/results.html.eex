<div>
  <h2>Search Results for <%= @query %></h2>

  <h3>New results</h3>
  <ol>
    <%= for result <- @new_results do %>
      <li>
        <a href="<%= result.url %>"><%= result.title %></a>
      </li>
    <% end %>
  </ol>

  <h3>Existing results</h3>
  <ol>
    <%= for result <- @existing_results do %>
      <li>
        <a href="<%= result.url %>"><%= result.title %></a>
      </li>
    <% end %>
  </ol>
</div>
